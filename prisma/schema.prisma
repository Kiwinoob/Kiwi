generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id           String   @id @default(cuid())
  title        String
  description  String
  image        String
  technologies String[]
  link         String
  status       String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

