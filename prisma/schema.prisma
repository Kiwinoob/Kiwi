generator client {
  provider = "prisma-client"
  output   = "./generated/v7"
}

datasource db {
  provider = "postgresql"
  relationMode = "prisma"
}

model Project {
  id           String   @id @default(cuid())
  title        String
  description  String
  image        String
  technologies String[]
  link         String
  status       String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

